        "icon": "$(clippy)"
      },
      {
        "command": "starcode-snippets.editSnippet",
        "title": "ÁºñËæë‰ª£Á†Å",
        "category": "StarCode Snippets",
        "icon": "$(notebook-open-as-text)"
      },
      {
        "command": "starcode-snippets.delete",
        "title": "Âà†Èô§",
        "category": "StarCode Snippets",
        "icon": "$(trash)"
      },
      {
        "command": "starcode-snippets.moveToDirectory",
        "title": "ÁßªÂä®Âà∞ÁõÆÂΩï",
        "category": "StarCode Snippets",
        "icon": "$(move)"
      },
      {
        "command": "starcode-snippets.createSnippetInDirectory",
        "title": "Êñ∞Âª∫‰ª£Á†ÅÁâáÊÆµ",
        "category": "StarCode Snippets",
        "icon": "$(add)"
      },
      {
        "command": "starcode-snippets.finishEditing",
        "title": "ÂÆåÊàêÁºñËæë",
        "category": "StarCode Snippets",
        "icon": "$(check)"
      },
      {
        "command": "starcode-snippets.previewSnippet",
        "title": "È¢ÑËßà‰ª£Á†ÅÁâáÊÆµ",
        "category": "StarCode Snippets",
        "icon": "$(eye)"
      },
      {
        "command": "starcode-snippets.exportSnippet",
        "title": "ÂØºÂá∫‰ª£Á†ÅÁâáÊÆµ",
        "category": "StarCode Snippets",
        "icon": "$(link-external)"
      },
      {
        "command": "starcode-snippets.exportAll",
        "title": "ÂØºÂá∫ÊâÄÊúâ‰ª£Á†ÅÁâáÊÆµ",
        "category": "StarCode Snippets",
        "icon": "$(link-external)"
      },
      {
        "command": "starcode-snippets.importSnippets",
        "title": "ÂØºÂÖ•‰ª£Á†ÅÁâáÊÆµ",
        "category": "StarCode Snippets",
        "icon": "$(log-in)"
      },
      {
        "command": "starcode-snippets.searchSnippets",
        "title": "ÊêúÁ¥¢‰ª£Á†ÅÁâáÊÆµ",
        "category": "StarCode Snippets",
        "icon": "$(search)"
      },
      {
        "command": "starcode-snippets.clearSearch",
        "title": "Ê∏ÖÈô§ÊêúÁ¥¢",
        "category": "StarCode Snippets",
        "icon": "$(clear-all)"
      },
      {
        "command": "starcode-snippets.toggleSearchMode",
        "title": "ÂàáÊç¢ÊêúÁ¥¢Ê®°Âºè",
        "category": "StarCode Snippets",
        "icon": "$(settings-gear)"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "starcode-snippets.saveToLibrary",
          "when": "editorHasSelection",
          "group": "9_cutcopypaste"
        }
      ],
      "editor/title": [
        {
          "command": "starcode-snippets.finishEditing",
          "group": "navigation",
          "when": "starcode-snippets.isEditingSnippet"
        }
      ],
      "view/title": [
        {
          "command": "starcode-snippets.searchSnippets",
          "when": "view == copyCodeExplorer",
          "group": "navigation@1"
        },
        {
          "command": "starcode-snippets.clearSearch",
          "when": "view == copyCodeExplorer && starcode-snippets.hasActiveSearch",
          "group": "navigation@2"
        },
        {
          "command": "starcode-snippets.toggleSearchMode",
          "when": "view == copyCodeExplorer",
          "group": "navigation@3"
        },
        {
          "command": "starcode-snippets.refreshExplorer",
          "when": "view == copyCodeExplorer",
          "group": "navigation@4"
        },
        {
          "command": "starcode-snippets.createDirectory",
          "when": "view == copyCodeExplorer",
          "group": "navigation@5"
        },
        {
          "command": "starcode-snippets.importSnippets",
          "when": "view == copyCodeExplorer",
          "group": "navigation@6"
        },
        {
          "command": "starcode-snippets.exportAll",
          "when": "view == copyCodeExplorer",
          "group": "navigation@7"
        }
      ],
      "view/item/context": [
        {
          "command": "starcode-snippets.createSnippetInDirectory",
          "when": "view == copyCodeExplorer && viewItem == directory",
          "group": "inline@1"
        },
        {
          "command": "starcode-snippets.rename",
          "when": "view == copyCodeExplorer && viewItem == directory",
          "group": "inline@2"
        },
        {
          "command": "starcode-snippets.delete",
          "when": "view == copyCodeExplorer && viewItem == directory",
          "group": "inline@3"
        },
        {
          "command": "starcode-snippets.createSnippetInDirectory",
          "when": "view == copyCodeExplorer && viewItem == directory",
          "group": "1_creation@1"
        },
        {
          "command": "starcode-snippets.previewSnippet",
          "when": "view == copyCodeExplorer && viewItem == snippet",
          "group": "inline@1"
        },
        {
          "command": "starcode-snippets.editSnippet",
          "when": "view == copyCodeExplorer && viewItem == snippet",
          "group": "inline@2"
        },
        {
          "command": "starcode-snippets.appendCode",
          "when": "view == copyCodeExplorer && viewItem == snippet",
          "group": "inline@3"
        },
        {
          "command": "starcode-snippets.moveToDirectory",
          "when": "view == copyCodeExplorer && viewItem == snippet",
          "group": "inline@4"
        },
        {
          "command": "starcode-snippets.rename",
          "when": "view == copyCodeExplorer && viewItem == snippet",
          "group": "inline@5"
        },
        {
          "command": "starcode-snippets.exportSnippet",
          "when": "view == copyCodeExplorer && viewItem == snippet",
          "group": "inline@6"
        },
        {
          "command": "starcode-snippets.delete",
          "when": "view == copyCodeExplorer && viewItem == snippet",
          "group": "inline@7"
        },
        {
          "command": "starcode-snippets.appendCode",
          "when": "view == copyCodeExplorer && viewItem == snippet",
          "group": "3_snippetactions@1"
        },
        {
          "command": "starcode-snippets.editSnippet",
          "when": "view == copyCodeExplorer && viewItem == snippet",
          "group": "3_snippetactions@2"
        },
        {
          "command": "starcode-snippets.previewSnippet",
          "when": "view == copyCodeExplorer && viewItem == snippet",
          "group": "3_snippetactions@3"
        },
        {
          "command": "starcode-snippets.moveToDirectory",
          "when": "view == copyCodeExplorer && viewItem == snippet",
          "group": "4_organization@1"
        },
        {
          "command": "starcode-snippets.exportSnippet",
          "when": "view == copyCodeExplorer && viewItem == snippet",
          "group": "5_export@1"
        }
      ]
    },
    "capabilities": {
      "untrustedWorkspaces": {
        "supported": true
      },
      "virtualWorkspaces": true,
      "treeItemButtons": true
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --fix",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.15.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.19.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1"
  },
  "dependencies": {
    "monaco-editor": "^0.52.2",
    "uuid": "^11.1.0"
  }
}